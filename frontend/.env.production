# React Frontend Environment Variables for Custom Domain
# This will be automatically used by React for production builds

# API URL for custom domain
REACT_APP_API_URL=https://api.vikrahub.com/api/

# App Environment
NODE_ENV=production

# Build optimization
GENERATE_SOURCEMAP=false
CI=false

# Custom domain specific settings
PUBLIC_URL=https://vikrahub.com

# Cloudinary Configuration (for direct uploads from frontend)
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_CLOUDINARY_API_KEY=your-cloudinary-api-key
