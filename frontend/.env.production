# React Frontend Environment Variables
# This will be automatically used by React for production builds

# API URL - Update this with your backend Render URL
REACT_APP_API_URL=https://your-backend-app.onrender.com/api/

# Cloudinary Configuration (for direct uploads from frontend)
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
REACT_APP_CLOUDINARY_API_KEY=your-cloudinary-api-key
